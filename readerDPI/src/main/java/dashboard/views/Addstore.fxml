<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="dashboard.controller.AddStoreController">
    <children>
        <VBox alignment="CENTER" layoutX="100.0" layoutY="50.0" spacing="10.0">
            <children>
                <Label text="ADD NEW STORE" />
                <TextField fx:id="storeIdField" promptText="Store ID" />
                <TextField fx:id="storeNameField" promptText="Store Name" />
                <TextField fx:id="addressField" promptText="Address" />
                <TextField fx:id="phoneNumberField" promptText="Phone Number" />
                <TextField fx:id="latitudeField" promptText="Latitude" />
                <TextField fx:id="longitudeField" promptText="Longitude" />
                <TextField fx:id="domainIdField" editable="false" promptText="Domain ID" />
                <Button onAction="#saveButtonClicked" text="Save" />
            </children>
        </VBox>
    </children>
</AnchorPane>
