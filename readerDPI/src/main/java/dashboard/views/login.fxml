<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="200" prefWidth="300" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dashboard.controller.LoginController">
    <children>
        <Label layoutX="114.0" layoutY="58.0" text="Đăng nhập"/>
        <Label layoutX="34.0" layoutY="94.0" text="Username"/>
        <TextField fx:id="usernameField" layoutX="99.0" layoutY="90.0"/>
        <Label layoutX="36.0" layoutY="126.0" text="Password"/>
        <PasswordField fx:id="passwordField" layoutX="99.0" layoutY="122.0"/>
        <Button fx:id="loginButton" layoutX="124.0" layoutY="162.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Đăng nhập"/>
        <Label fx:id="statusLabel" layoutX="89.0" layoutY="196.0"/>
    </children>
</AnchorPane>
